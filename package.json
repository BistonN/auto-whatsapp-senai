  "bugs": {
    "url": "https://github.com/BistonN/auto-whatsapp-senai/issues"
  },
  "homepage": "https://github.com/BistonN/auto-whatsapp-senai#readme",
  "devDependencies": {
    "electron": "^16.0.5",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.6.0"
  },
  "build": {
    "extraResources": [
      "./extraResources/**"
    ]
  }
